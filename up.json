{
	"name": "typhlopidae",
	"profile": "default",
	"regions": ["ap-northeast-1"],
	"type": "static",
	"static": {
		"dir": "build"
	},
	"hooks": {
		"build": "node scripts/build.js"
	},
	"redirects": {
		"/*": {
			"location": "/",
			"status": 200
		}
	},
	"stages": {
		"production": {
			"path": "/",
			"domain": "vip.asobo.name"
		}
	}
}
